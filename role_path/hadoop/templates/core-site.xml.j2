<configuration>
     <property>
         <name>fs.defaultFS</name>
         <value>hdfs://localhost:9000</value>
     </property>
     <property>
         <name>hadoop.tmp.dir</name>
         <value>/Development/Hadoop/hadoop/tmp</value>
     </property>
     <property>
         <name>hadoop.proxyuser.{{ ansible_hostname }}.groups</name>
